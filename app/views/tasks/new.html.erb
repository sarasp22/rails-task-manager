<div class="container" id="form">
<h1>New Task</h1>
<div class="form-container">
<%= form_with model: @task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :completed %>
  <%= f.check_box :completed %>
  <%= f.submit %>
<% end %>
</div>

<div class="button-51" id="back">
<%= link_to("Back", index_path) %>
</div>
</div>
